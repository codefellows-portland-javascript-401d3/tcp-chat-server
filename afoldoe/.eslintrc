{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
        "jsx": true
        }
    },
    "env": {
        "browser": true,
        "jquery": true,
        "es6": true
    },
    "rules": {
        "semi": 1,
        "strict": 0,
        "indent": [2, 2],
        "no-multi-spaces": [1, {
            "exceptions": {
                "VariableDeclarator": true,
                "FunctionExpression": true
            }
        }],
        "key-spacing": [0, {"align": "value"}],
        "no-underscore-dangle": 0
    }
}